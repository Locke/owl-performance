Permissions of this strongest copyleft license are conditioned on making available complete source code of licensed works and modifications, which include larger works using a licensed work, under the same license. Copyright and license notices must be preserved. Contributors provide an express grant of patent rights. When a modified version is used to provide a service over a network, the complete source code of the modified version must be made available.</rdfs:comment>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled -->

    <owl:AnnotationProperty rdf:about="http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#hasBase -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#hasBase">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#hasIngredient"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#isBaseOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#Pizza"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#PizzaBase"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#hasIngredient -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#hasIngredient">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#isIngredientOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#hasSpiciness -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#hasSpiciness">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#PizzaTopping"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#Spiciness"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#hasSpicinessPreference -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#hasSpicinessPreference">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#Customer"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#Spiciness"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#hasTopping -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#hasTopping">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#hasIngredient"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#isToppingOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#Pizza"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#PizzaTopping"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#isBaseOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#isBaseOf">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#isIngredientOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#isIngredientOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#isIngredientOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#isMilderThan -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#isMilderThan">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#isSpicierThan"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#isSpicierThan -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#isSpicierThan">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#Spiciness"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#Spiciness"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#isToppingOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#isToppingOf">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#isIngredientOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#purchasedByCustomer -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#purchasedByCustomer">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#purchasedPizza"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#purchasedPizza -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#purchasedPizza">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#Customer"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#Pizza"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#hasCaloricContent -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#hasCaloricContent">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#Pizza"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#hasDiscount -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#hasDiscount">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#Customer"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#hasPhone -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#hasPhone">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#Person"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#numberOfPizzasPurchased -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#numberOfPizzasPurchased">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#Customer"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#ssn -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#ssn">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#Employee"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#AmericanaHotPizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#AmericanaHotPizza">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#NamedPizza"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#JalapenoPepperTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#MozzarellaTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#PepperoniTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#TomatoTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A pizza that only has Mozzarella, Tomato, Jalapeno, and Pepperoni toppings</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#AmericanaPizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#AmericanaPizza">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#NamedPizza"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#MozzarellaTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#PepperoniTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#TomatoTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A pizza that only has Mozzarella, Tomato and Pepperoni toppings</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#AnchovyTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#AnchovyTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#SeafoodTopping"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#CaperTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#CaperTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#VegetableTopping"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#hasSpiciness"/>
                <owl:hasValue rdf:resource="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#Mild"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#CheeseTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#CheeseTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#PizzaTopping"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#hasSpiciness"/>
                <owl:hasValue rdf:resource="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#Mild"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#CheesyPizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#CheesyPizza">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#Pizza"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#hasTopping"/>
                        <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#CheeseTopping"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#Customer -->

    <owl:Class rdf:about="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#Customer">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#Person"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#purchasedPizza"/>
                        <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#Pizza"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#hasPhone"/>
                        <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#DeepPanBase -->

    <owl:Class rdf:about="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#DeepPanBase">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#PizzaBase"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#ThinAndCrispyBase"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#Employee -->

    <owl:Class rdf:about="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#Employee">
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#Person"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#ssn"/>
                        <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                        <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#GreenPepperTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#GreenPepperTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#PepperTopping"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#hasSpiciness"/>
                <owl:hasValue rdf:resource="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#Medium"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#HamTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#HamTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#MeatTopping"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#hasSpiciness"/>
                <owl:hasValue rdf:resource="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#Mild"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#HighCaloriePizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#HighCaloriePizza">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#Pizza"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#hasCaloricContent"/>
                        <owl:someValuesFrom>
                            <rdfs:Datatype>
                                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
                                <owl:withRestrictions rdf:parseType="Collection">
                                    <rdf:Description>
                                        <xsd:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">400</xsd:minInclusive>
                                    </rdf:Description>
                                </owl:withRestrictions>
                            </rdfs:Datatype>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#HotVeggiePizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#HotVeggiePizza">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/pizzatutorial/ontologies/2020/PizzaTutorialWithoutData#VegetarianPizza"/>
                    <owl:Restriction>
